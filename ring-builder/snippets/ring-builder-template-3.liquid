<!-- SETTING MEDIA GALLERY -->
<script
  type="text/x-template"
  id="setting-media-gallery"
>
    <div class="new pc-details-media-gallery-div pc-flex pc-w-full pc-flex-col pc-items-center pc-gap-y-30 pc-px-20">        
        <div class="pc-flex pc-flex-col pc-items-center pc-w-9/10 pc-gap-y-20 pc-detail-container-setting-media">
            <div class="pc-settings-image-inner-container" v-if="!changeMetalOrDiamondLoading">
                <span class="pc-loader" v-if="showLoader"></span>
                <div class="product-grid">
                  <!-- Loop through mediaGallery or any dynamic data -->
                  <template v-for="(item, index) in mediaGallery?.images" :key="index">
                    <div class="product-grid-item">
                      <!-- Image rendering logic -->
                      <img 
                        v-if="item.src"
                        :src="item.src" 
                        :alt="item.caption"
                        width="100" 
                        height="100" 
                        class="pc-slide-img" 
                        class="pc-w-full"  
                        @error="setPlaceholderImg($event.target)" 
                        @mouseover="showZoomIcon = true, zoomImgUrl = item.src" 
                        @load="showLoader = false" />
                      
                      <!-- Video rendering logic -->
                      {% comment %} <iframe
                        v-if="item.videoUrl"
                        width="560"
                        height="315"
                        :src="item.videoUrl || ''"
                        title="Youtube Player"
                        frameborder="0"
                      />
                      
                      <video
                        v-if="item.videoUrl && !showLoader && gallery.name == 'video' && index == gallery.pos"
                        :poster="showLoader"
                        autoplay
                        muted
                        loop
                        width="640"
                        height="640"
                        :class="{'pc-w-1/10': showLoader, 'pc-visibility-hidden': showLoader}"
                        @mouseover="showZoomIcon = false"
                      >
                        <source :src="item.videoUrl" type="video/mp4" @error="handleVideoError" />
                      </video> {% endcomment %}
                    </div>
                  </template>
                </div>
              </div>
        </div>
    </div>
    <teleport to="#popup-container">
        <full-img-popup ref="fullImg" v-show="showPopupImg" @close-img-popup="showPopupImg = false, zoomImgUrl = '' " :class="{'tps_large_img' : showPopupImg}"></full-img-popup>
    </teleport>
</script>
<!-- END SETTING MEDIA GALLERY -->

