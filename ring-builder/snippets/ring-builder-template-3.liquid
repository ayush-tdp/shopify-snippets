<!-- DIAMOND CARAT GRAPHIC COMPONENT -->
<script
  type="text/x-template"
  id="diamond-carat-graphic"
>
    <div class="pc-dia-graphics pc-px-20">
        <div class="container carat_information pc-pb-40 pc-flex pc-flex-col pc-gap-y-5">   
            <div class="title pc-bold pc-text-04043F pc-my-10 pc-transform-none">Carat</div>
            <div class="sub-title pc-text-04043F pc-my-10"><span>Selected Carat:</span> <span class="pc-text-04043F pc-bold">[[`${parseFloat(diamond.caratWeight)} Ct.`]]</span></div>
            <div class="carat_info info">
                <p class="pc-text-04043F pc-graphic-desc" v-html="getCaratTitle"></p>
            </div>        
            <div class="pc-diamond_box pc-w-full">
                <div class="arrow_box pc-indicator-box pc-py-10 pc-px-30 pc-rounded-sm" :style="{left : `${getCaratPosition(parseFloat(diamond.caratWeight))}%`}">
                    <div class="pc-w-full pc-max-h-90 pc-overflow-y-auto pc-overflow-x-hidden">
                        <div class="pc-popover-text pc-text-white">[[`${parseFloat(diamond.caratWeight)} Ct.`]]</div>
                    </div>
                </div>
                <div class="pc-w-full pc-flex pc-justify-center pc-items-center">
                    <img :src="getCaratMeterGraphicImgUrl" class="img-responsive pc-w-86" />
                </div>
            </div>    
        </div>
    </div>
</script>
